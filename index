    <script>
        $(function(){$('input[type="checkbox"]').click(function(){ var notCHECK = document.querySelectorAll('input[type="checkbox"]:not(:checked)');var notLIST = [];for( i = 0 ; i < notCHECK.length ; i++ ){notLIST.push( notCHECK[i].id.toUpperCase() )}console.log( '非表示',notLIST );$('.chat').removeClass('hide');for( i = 0 ; i < notLIST.length ; i++ ){$('.chat.' + notLIST[i] ).addClass('hide');}});});
        function tabHIDE(){var btn = document.getElementById('tabBTN');var isSHOW = btn.className;if( isSHOW == 'show' ){$('input[type="checkbox"][name="tab"]').prop('checked', false);$('.chat').addClass('hide');btn.classList.remove('show');var btnTxt = 'Show all <i class="fa-solid fa-angle-right"></i>';}else{$('input[type="checkbox"][name="tab"]').prop('checked', true);$('.chat').removeClass('hide');$('#dummy').trigger('click');btn.classList.add('show');var btnTxt = 'Hide all <i class="fa-solid fa-chevron-left"></i>';}$('#tabBTN').html(btnTxt);}
        function memHIDE(){var btn = document.getElementById('memBTN');var isSHOW = btn.className;if( isSHOW == 'show' ){$('input[type="checkbox"][name="mem"]').prop('checked', false);$('.chat').addClass('hide');btn.classList.remove('show');var btnTxt = 'Show all <i class="fa-solid fa-angle-right"></i>';}else{$('input[type="checkbox"][name="mem"]').prop('checked', true);$('.chat').removeClass('hide');$('#dummy').trigger('click');btn.classList.add('show');var btnTxt = 'Hide all <i class="fa-solid fa-chevron-left"></i>';}$('#memBTN').html(btnTxt);}
        $(function(){$('summary').click(function(){$(this).toggleClass('open');if( $('details').prop('open') ){$('body').removeClass('noscroll');}else{$('body').addClass('noscroll');}});});
        $(function(){$('.itemToggleMenu').click(function(){var showTgt = '.showMenuContents[name="' + $(this).attr('id') + '"]';if($(showTgt).css('display') == 'block'){return;}$('.itemToggleMenu').removeClass('thisOn');$(this).addClass('thisOn');$('.showMenuContents').hide();$(showTgt).show(200);});});
        $(function(){$('.dice').click(function(){var notchecked = document.querySelectorAll('input[type="checkbox"]:not(:checked)');var del = [];for( i = 0 ; i < notchecked.length ; i++ ){del.push( notchecked[i].id.toUpperCase() )}console.log(del);$('#dice-area').removeClass('none');$('#dice-area').html('<div id="loading"><i class="fa-solid fa-spinner"></i></div>');var id = $(this).attr('id');var tabALL = [];var tabID = [];$('label[for*="tab"]').each(function(){tabALL.push($(this).html());tabID.push($(this).prev().attr('id').toUpperCase());});var member = [];var memID = [];$('label[for*="mem"]').each(function(){member.push($(this).html());memID.push($(this).prev().attr('id').toUpperCase());});var dicelog = [];var diceMEM = [];var diceTAB = [];$('.chat').each(function(){var thisTEXT = $(this).text();var isDICE = thisTEXT.match(/1D100/) && thisTEXT.match(/\<=/);if( isDICE && (thisTEXT.match(/成功|失敗|スペシャル/)) ){var stop = false;var hasCLASS = $(this).attr('class').split(' ');for( var cls = 0 ; cls < hasCLASS.length ; cls++ ){if( del.indexOf( hasCLASS[cls] ) !== -1 ){stop = true;break;}}if( stop ){ return true;}dicelog.push($(this).html().split('</b>')[1]);diceMEM.push($(this).attr('class').split(' ')[2]);diceTAB.push($(this).attr('class').split(' ')[1]);}});var outputHTML = '';var cfRESULT = '';for(i = 0 ; i < member.length ; i++){if(i !== 0){ if( outputHTML.slice(-8) == '</b><br>' ){outputHTML += '-<br>';}else{cfRESULT += '<b>【' + member[ i - 1 ] + '】 </b>' + critical + 'C' + fumble + 'F<br>';}outputHTML += '<br><b>▼ ' + member[i] + '</b><br>';}else{ outputHTML += '<b>▼ ' + member[i] + '</b><br>'; }var critical = 0;var fumble = 0;for(j = 0 ; j < dicelog.length ; j++){try{ var tabNAME = tabALL[ tabID.indexOf( diceTAB[j] ) ];if( tabNAME == undefined ){ throw new Error(); }}catch(e){ var tabNAME = 'タブ名判定エラー'; }var diceRESULT = dicelog[j];try{diceRESULT = diceRESULT.trim();}catch(e){;}if(id == 'all'){if(diceMEM[j] == memID[i]){outputHTML += '［' + tabNAME + '］' + diceRESULT + '<br>';}}else if(id == 'scs'){if( dicelog[j].match(/成功|スペシャル/) ){if(diceMEM[j] == memID[i]){outputHTML += '［' + tabNAME + '］' + diceRESULT + '<br>';}}}else if(id == 'fail'){if( dicelog[j].match(/失敗/) ){if(diceMEM[j] == memID[i]){outputHTML += '［' + tabNAME + '］' + diceRESULT + '<br>';}}}else if(id == 'cf'){if( dicelog[j].match(/決定的成功|致命的失敗/) ){if(diceMEM[j] == memID[i]){outputHTML += '［' + tabNAME + '］' + diceRESULT + '<br>';if( dicelog[j].match(/決定的成功/) ){ critical += ( dicelog[j].match( /決定的成功/g ) || [] ).length; }if( dicelog[j].match(/致命的失敗/) ){ fumble += ( dicelog[j].match( /致命的失敗/g ) || [] ).length;}}}}}}if( outputHTML.slice(-8) == '</b><br>' ){outputHTML += '-<br>';}else{cfRESULT += '<b>【' + member[ member.length - 1 ] + '】 </b>' + critical + 'C' + fumble + 'F<br>';}outputHTML = outputHTML.split('<b>');for( i = 0 ; i < outputHTML.length ; i++ ){if( outputHTML[i].match(/<br>-<br>/) ){outputHTML[i] = '</b>';}}outputHTML = outputHTML.join('<b>');outputHTML = outputHTML.replace(/<b><\/b>/g,'');$('#dice-area').html( outputHTML );$('.fa-copy').removeClass('none');if( id == 'cf' ){if(cfRESULT == ''){ cfRESULT = 'CFなし'; }$('#cf-area').html( cfRESULT );$('#cf-area').removeClass('none');}else{$('#cf-area').addClass('none');}});});
        $(function(){$('.fa-copy').click(function(){var txt = $('#dice-area').html();txt = txt.split('<b>').join('');txt = txt.split('</b>').join('');try{ txt = txt.replace(/&lt;/g,'<'); }catch(e){;}try{ txt = txt.replace(/&nbsp;/g,' '); }catch(e){;}try{ txt = txt.replace(/\n			#/g,'#') }catch(e){;}txt = txt.split('<br>').join('\n');copyToClipboard(txt);alert('クリップボードにコピーしました。\n\n- - - - - - - - - - - - - - - - - - - - - - - \n' + txt);});});
        function copyToClipboard(txt){const pre = document.createElement('pre');pre.style.webkitUserSelect = 'auto';pre.style.userSelect = 'auto';pre.textContent = txt;document.body.appendChild(pre);document.getSelection().selectAllChildren(pre);const result = document.execCommand('Copy');document.body.removeChild(pre);return result;}
    </script>
